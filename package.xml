<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<!-- do not use the "Type" attribute here, that one is only for
     generated package.xml files -->
<package version="1.0">
  <name>Net_Finger</name>
  <summary>The PEAR::Net_Finger class provides a tool for querying Finger Servers</summary>
  <description>
   Wrapper class for finger calls.
  </description>
  <maintainers>
    <maintainer>
      <user>nohn</user>
      <name>Sebastian Nohn</name>
      <email>sebastian@nohn.net</email>
    </maintainer>
  </maintainers>
  <license>PHP License</license>
  <release>
    <version>1.0.0</version>
    <state>stable</state>
    <date>2002-01-10</date>
    <notes>
      This is the initial independent release of the Net_Finger package.
    </notes>
    <filelist>
      <dir name="/" baseinstalldir="Net">
        <file role="php">Finger.php</file>
      </dir>
    </filelist>
    <deps>
      <dep type="pkg" rel="has">Net_Socket</dep>
    </deps>
  </release>
